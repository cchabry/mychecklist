
{
  "version": 2,
  "functions": {
    "api/*.ts": {
      "memory": 512,
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/api/ping",
      "dest": "/api/ping.ts",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/notion-proxy",
      "dest": "/api/notion-proxy.ts",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
}
