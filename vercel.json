
{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/notion-proxy",
      "dest": "/api/notion-proxy.ts"
    },
    {
      "src": "/api/ping",
      "dest": "/api/ping.ts"
    },
    {
      "src": "/api/vercel-debug",
      "dest": "/api/vercel-debug.ts"
    }
  ]
}
